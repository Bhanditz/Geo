{
	"name": "data-values/geo",
	"type": "library",
	"description": "Geographical value objects, parsers and formatters",
	"keywords": [
		"datavalues",
		"parsers",
		"formatters",
		"wikidata",
		"geo",
		"geographical",
		"latlong"
	],
	"homepage": "https://github.com/DataValues/Geo",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Jeroen De Dauw",
			"email": "jeroendedauw@gmail.com",
			"homepage": "http://jeroendedauw.com",
			"role": "Developer"
		},
		{
			"name": "The Wikidata team",
			"role": "Developer"
		}
	],
	"support": {
		"irc": "irc://irc.freenode.net/wikidata"
	},
	"require": {
		"php": ">=7.1",
		"data-values/data-values": "~2.0|~1.0|~0.1",
		"data-values/interfaces": "^0.2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~6.0",
		"wikibase/wikibase-codesniffer": "^0.4.0",
		"ockcyp/covers-validator": "~0.6"
	},
	"autoload": {
		"psr-4": {
			"DataValues\\Geo\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\DataValues\\Geo\\": "tests/unit"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.0.x-dev"
		}
	},
	"scripts": {
		"test": [
			"composer validate --no-interaction",
			"phpcs -p -s",
			"covers-validator",
			"phpunit"
		]
	}
}
